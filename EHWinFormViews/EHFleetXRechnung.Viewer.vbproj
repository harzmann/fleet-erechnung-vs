<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace>EHFleetXRechnung.Viewer</RootNamespace>
    <TargetFramework>net4.8</TargetFramework>
	  <UseWindowsForms>true</UseWindowsForms>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Icons\approval.svg" />
    <None Remove="Icons\car-repair.svg" />
    <None Remove="Icons\file.svg" />
    <None Remove="Icons\garage.svg" />
    <None Remove="Icons\gasoline.svg" />
    <None Remove="Icons\pdf.svg" />
    <None Remove="Icons\trust.svg" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Icons\approval.svg" />
    <EmbeddedResource Include="Icons\car-repair.svg" />
    <EmbeddedResource Include="Icons\file.svg" />
    <EmbeddedResource Include="Icons\garage.svg" />
    <EmbeddedResource Include="Icons\gasoline.svg" />
    <EmbeddedResource Include="Icons\pdf.svg" />
    <EmbeddedResource Include="Icons\trust.svg" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ehfleet_classlibrary" Version="1.3.6">
    </PackageReference>
    <PackageReference Include="log4net" Version="3.0.1" />
	<PackageReference Include="Stimulsoft.Reports.Net" Version="2024.4.1">
	</PackageReference>
    <PackageReference Include="UI.for.WinForms.AllControls.Net48" Version="2024.3.924">
    </PackageReference>
  </ItemGroup>


	<ItemGroup>
	  <ProjectReference Include="..\ZUGFeRD\ZUGFeRD.csproj" />
	</ItemGroup>
	<ItemGroup>
		<Reference Include="netstandard" />
		<Reference Include="mscorlib" />
	</ItemGroup>
	<ItemGroup>
	  <Compile Update="My Project\Resources.Designer.vb">
	    <DesignTime>True</DesignTime>
	    <AutoGen>True</AutoGen>
	    <DependentUpon>Resources.resx</DependentUpon>
	  </Compile>
	</ItemGroup>
	<ItemGroup>
	  <EmbeddedResource Update="My Project\Resources.resx">
	    <CustomToolNamespace>My.Resources</CustomToolNamespace>
	    <Generator>VbMyResourcesResXFileCodeGenerator</Generator>
	    <LastGenOutput>Resources.Designer.vb</LastGenOutput>
	  </EmbeddedResource>
	</ItemGroup>
	<ItemGroup>
	  <None Update="license.key">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </None>
	</ItemGroup>
</Project>
